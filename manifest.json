{
  "name": "LivePage",
  "version": "1.3.0.0",
  "manifest_version": 1,
  
  "description": "LivePage reloads website resources (such as CSS, LESS, HTML and JavaScript) as they change on the server.",
  "background_page": "background.html",
  "options_page": "options.html",
  "homepage_url": "https://chrome.google.com/webstore/detail/pilnojpmdoofaelbinaeodfpjheijkbh",
  "permissions": [
	"tabs", 
	"http://*/*", 
	"https://*/*",
	"file://*/*"
  ],
  "browser_action": {
    "default_title": "Enable LivePage on this page",
    "default_icon": "icon.png"
  },
  "icons": {
  	"16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "minimum_chrome_version": "20.0",
  "offline_enabled": true
}